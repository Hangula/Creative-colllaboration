<%= simple_form_for @project do |f|	 %>



	<%= f.input :title, label: "Project Title" %>

	<%= f.input :description, label: "description"%>

	<%= f.input :budget, label: "Project budget"%>

	<%= f.input :company, label: "Company"%>

	<%= f.input :start_date, label: "Starting date"%>

	<%= f.input :due_date, label: "Project due date"%>

	<%= f.input :hustle_points, label: "Hustle Points"%>


	<%= f.button :submit %>

<% end %> 

 